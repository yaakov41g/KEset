{
  "name": "KEset | Fix gibberish from wrong keyboard layout",
  "description": "Correcting wrong language typing with a short-cut",
  "version": "1.3",
  "options_page": "options.html",
  "permissions": [ "storage" ],
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "hello_extensions16.png",
      "32": "hello_extensions32.png",
      "48": "hello_extensions48.png",
      "128": "hello_extensions128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "jquery-3.5.1.min.js", "contentscript.js" ],
      "css": [ "extension.css" ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "hello_extensions16.png",
    "32": "hello_extensions32.png",
    "48": "hello_extensions48.png",
    "128": "hello_extensions128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Correct gibberish"
    }
  }
}
